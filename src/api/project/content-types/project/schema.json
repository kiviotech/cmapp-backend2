{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "end_date": {
      "type": "date"
    },
    "start_date": {
      "type": "date"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "project"
    },
    "project_type": {
      "type": "string"
    },
    "location": {
      "type": "text"
    },
    "contractors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::contractor.contractor",
      "mappedBy": "projects"
    },
    "project_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "ongoing",
        "ahead",
        "delayed",
        "completed"
      ]
    },
    "documents": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "approver": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-team.project-team",
      "inversedBy": "projects"
    }
  }
}
